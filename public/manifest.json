{
  "name": "Cookie Monster",
  "description": "Devours your cookies",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open Cookie Monster"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}